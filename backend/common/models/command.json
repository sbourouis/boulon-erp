      "type": "date",
      "required": true
    },
    "discount": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": "",
      "options": {
        "nestRemoting": true,
        "disableInclude": true
      }
    },
    "commandLines": {
      "type": "hasMany",
      "model": "CommandLine",
      "foreignKey": "idCommandLines",
      "options": {
        "nestRemoting": true,
        "disableInclude": true
      }
    }
  },
  "acls": [],
  "methods": {}
