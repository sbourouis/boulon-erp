{
  "name": "Command",
  "plural": "commands",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "date": {
      "type": "date",
      "required": true
    },
    "quantity": {
      "type": "number",
      "required": true,
      "default": 1000
    }
  },
  "validations": [],
  "relations": {
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": "idCustomer",
      "options": {
        "nestRemoting": true,
        "disableInclude": true
      }
    },
    "products": {
      "type": "hasOne",
      "model": "Product",
      "foreignKey": "idProduct",
      "options": {
        "nestRemoting": true,
        "disableInclude": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
