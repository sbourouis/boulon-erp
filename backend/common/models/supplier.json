{
  "name": "Supplier",
  "plural": "suppliers",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "type": {
      "type": "string"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string"
    },
    "phoneNumber": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "position": {
      "type": "string"
    },
    "comment": {
      "type": "string"
    },
    "deliveryTime": {
      "type": "number",
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "materials": {
      "type": "hasMany",
      "model": "Material",
      "foreignKey": "idMaterial",
      "options": {
        "nestRemoting": true,
        "disableInclude": true
      },
      "through": "Material"
    }
  },
  "acls": [],
  "methods": {}
}
